    "concurrently": "^3.5.1",
    "css-module": "^1.1.4",
    "express": "^4.16.2",
    "mongodb": "^3.1.4",
    "react": "^16.5.0",
    "react-helmet": "^5.2.0",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "webpack": "^4.17.2"
  }
}
